<h1>Listing mall_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Introtext</th>
    <th>Description</th>
    <th>Ispublished</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

  <% @mall_types.each do |mall_type| %>
      <tr>
        <td><%= mall_type.name %></td>
        <td><%= mall_type.introtext %></td>
        <td><%= mall_type.description %></td>
        <td><%= mall_type.ispublished %></td>
        <td><%= link_to 'Show', mall_type %></td>
        <td><%= link_to 'Edit', edit_mall_type_path(mall_type) %></td>
        <td><%= link_to 'Destroy', mall_type, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
  <% end %>
</table>

<br/>

