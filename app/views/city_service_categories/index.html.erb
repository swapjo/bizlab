<h1>Listing city_service_categories</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Introtext</th>
    <th>Description</th>
    <th>Ispublished</th>
    <th>Parent category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @city_service_categories.each do |city_service_category| %>
  <tr>
    <td><%= city_service_category.name %></td>
    <td><%= city_service_category.introtext %></td>
    <td><%= city_service_category.description %></td>
    <td><%= city_service_category.ispublished %></td>
    <td><%= city_service_category.parent_category_id %></td>
    <td><%= link_to 'Show', city_service_category %></td>
    <td><%= link_to 'Edit', edit_city_service_category_path(city_service_category) %></td>
    <td><%= link_to 'Destroy', city_service_category, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

